<h3>Bank Form</h3>

<div class="container">
  <form>
    <label for="fname">Bank Name</label>
    <input type="text" id="bankname" name="bankname" placeholder="Bank name.." required data-error="Please enter Bank Name">

    <label for="lname">Branch Name</label>
    <input type="text" id="branchname" name="branchname" placeholder="Branch name.." required data-error="Please enter Bank Branch">

    <label for="country">Address</label>
    <input type="text" id="address" name="address" placeholder="adress .." required data-error="Please enter Bank Address">

    <label for="subject">Bank Location</label>
    <input type="text" id="gpsaddress" name="gpsaddress" placeholder="enter location ex:29.973606927863575, 31.271875269263614 ..">

    <label for="ZoneName">ZoneName</label>
    <select name="ZoneName" id="ZoneName">
      <option value="Cairo">Cairo</option>
      <option value="Giza">Giza</option>
    </select>

    <div class="container">
        <button class="btn" onclick="insertbankinfo()">Submit</button>
    </div>
  </form>
</div>

</body>
</html>
